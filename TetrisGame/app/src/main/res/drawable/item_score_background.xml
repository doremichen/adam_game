<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#33000000">

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#FFFDF8E4"
                android:endColor="#FFF6E4AA"
                android:angle="90" />

            <corners android:radius="12dp" />

            <stroke
                android:width="1dp"
                android:color="#CCAA6600" />

            <padding
                android:left="6dp"
                android:top="6dp"
                android:right="6dp"
                android:bottom="6dp" />

        </shape>
    </item>

</ripple>
